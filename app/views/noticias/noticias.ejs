<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>Noticias</title>
		</head>
	<body>

		<h1>Noticias</h1>

		<table>
			<tr>
				<td>id</td>
				<td>titulo</td>
				<td>noticia</td>
			</tr>

			<% for(let i=0; i < noticias.length; i++) { %>
				<tr>
					<td><%= noticias[i].id_noticias  %></td>
					<td><%= noticias[i].titulo %></td>
					<td><%= noticias[i].noticia %></td>
				</tr>
			<% } %>
		</table>

	</body>
</html>